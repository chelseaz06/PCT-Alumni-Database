        // HTML escape function
        function escapeHtml(text) {
            const map = {
                '&': '&amp;',
                '<': '&lt;',
                '>': '&gt;',
                '"': '&quot;',
                "'": '&#039;'
            };
            return (text || '').replace(/[&<>"']/g, m => map[m]);
        }

        // Categorize industry based on company and role
        function categorizeIndustry(company, role) {
            const companyLower = (company || '').toLowerCase();
            const roleLower = (role || '').toLowerCase();
            const combined = companyLower + ' ' + roleLower;

            if (combined.includes('software') || combined.includes('engineer') || combined.includes('developer') || 
                combined.includes('amazon') || combined.includes('google') || combined.includes('meta') || 
                combined.includes('microsoft') || combined.includes('aws') || combined.includes('tesla') ||
                combined.includes('apple') || combined.includes('facebook') || combined.includes('linkedin')) {
                return 'Technology';
            }
            if (combined.includes('finance') || combined.includes('financial') || combined.includes('bank') || 
                combined.includes('capital') || combined.includes('investment') || combined.includes('analyst') ||
                combined.includes('jp morgan') || combined.includes('mood') || combined.includes('bmo') ||
                combined.includes('bain') || combined.includes('deloitte') || combined.includes('pwc') ||
                combined.includes('accounting') || combined.includes('audit')) {
                return 'Finance & Consulting';
            }
            if (combined.includes('marketing') || combined.includes('media') || combined.includes('advertising') ||
                combined.includes('social media') || combined.includes('brand')) {
                return 'Marketing & Media';
            }
            if (combined.includes('law') || combined.includes('attorney') || combined.includes('legal')) {
                return 'Law';
            }
            if (combined.includes('healthcare') || combined.includes('medical') || combined.includes('hospital') ||
                combined.includes('medicine') || combined.includes('dentistry')) {
                return 'Healthcare';
            }
            if (combined.includes('education') || combined.includes('school') || combined.includes('university') ||
                combined.includes('student') || combined.includes('grad school')) {
                return 'Education';
            }
            if (combined.includes('retail') || combined.includes('merchandise') || combined.includes('store') ||
                combined.includes('nike') || combined.includes('costco') || combined.includes('starbucks')) {
                return 'Retail & Consumer';
            }
            if (combined.includes('supply chain') || combined.includes('operations') || combined.includes('logistics') ||
                combined.includes('ocsm')) {
                return 'Operations & Supply Chain';
            }
            if (combined.includes('hr') || combined.includes('human resources') || combined.includes('people')) {
                return 'Human Resources';
            }
            if (combined.includes('product') || combined.includes('product manager') || combined.includes('pm')) {
                return 'Product Management';
            }
            if (combined.includes('consulting') || combined.includes('consultant') || combined.includes('accenture') ||
                combined.includes('west monroe')) {
                return 'Consulting';
            }
            return 'Other';
        }

        // Initialize
        let filteredData = [...alumniData];

        // Get unique majors and industries
        const majors = new Set();
        const industries = new Set();

        alumniData.forEach(alumni => {
            if (alumni['Undergraduate Major']) {
                const majorParts = alumni['Undergraduate Major'].split(',').map(m => m.trim());
                majorParts.forEach(major => {
                    if (major) majors.add(major);
                });
            }
            const industry = categorizeIndustry(alumni['Current Company'], alumni['Role']);
            industries.add(industry);
        });

        // Populate filter dropdowns
        const majorFilter = document.getElementById('majorFilter');
        const industryFilter = document.getElementById('industryFilter');

        Array.from(majors).sort().forEach(major => {
            const option = document.createElement('option');
            option.value = major;
            option.textContent = major;
            majorFilter.appendChild(option);
        });

        Array.from(industries).sort().forEach(industry => {
            const option = document.createElement('option');
            option.value = industry;
            option.textContent = industry;
            industryFilter.appendChild(option);
        });

        // Render alumni cards
        function renderAlumni(data) {
            const grid = document.getElementById('alumniGrid');
            const countElement = document.getElementById('resultCount');
            
            countElement.textContent = data.length;

            if (data.length === 0) {
                grid.innerHTML = '<div class="no-results">No alumni match your filters. Try adjusting your search criteria.</div>';
                return;
            }

            grid.innerHTML = data.map(alumni => {
                const major = alumni['Undergraduate Major'] || 'Not specified';
                const company = alumni['Current Company'] || 'Not specified';
                const role = alumni['Role'] || 'Not specified';
                const location = alumni['Current Location'] || '';
                const linkedin = alumni['LinkedIn'] || '';
                const gradYear = alumni['Grad Year'] || '';
                const coffeeChat = (alumni['Open to coffee chats?'] || '').toLowerCase();
                const isOpenToChat = coffeeChat === 'yes' || coffeeChat === 'online';
                const name = escapeHtml(alumni['Name'] || 'Unknown');

                // Fix LinkedIn URLs that don't start with http
                let linkedinUrl = linkedin;
                if (linkedinUrl && !linkedinUrl.startsWith('http')) {
                    linkedinUrl = 'https://' + linkedinUrl;
                }

                return `
                    <div class="alumni-card ${isOpenToChat ? 'open-to-chat' : ''}">
                        ${gradYear ? `<span class="grad-year">${escapeHtml(gradYear)}</span>` : ''}
                        <div class="alumni-name">${name}</div>
                        <div class="alumni-major">${escapeHtml(major)}</div>
                        ${company !== 'Not specified' ? `<div class="alumni-info"><strong>Company:</strong> ${escapeHtml(company)}</div>` : ''}
                        ${role !== 'Not specified' ? `<div class="alumni-info"><strong>Role:</strong> ${escapeHtml(role)}</div>` : ''}
                        ${location ? `<div class="alumni-location"> ${escapeHtml(location)}</div>` : ''}
                        ${isOpenToChat ? '<span class="coffee-chat-badge"> Open to Coffee Chats</span>' : ''}
                        ${linkedinUrl ? `<a href="${escapeHtml(linkedinUrl)}" target="_blank" rel="noopener noreferrer" class="alumni-linkedin">View LinkedIn Profile </a>` : ''}
                    </div>
                `;
            }).join('');
        }

        // Filter function
        function applyFilters() {
            const majorFilterValue = document.getElementById('majorFilter').value;
            const industryFilterValue = document.getElementById('industryFilter').value;
            const coffeeChatFilterValue = document.getElementById('coffeeChatFilter').value;

            filteredData = alumniData.filter(alumni => {
                // Major filter
                if (majorFilterValue) {
                    const majors = (alumni['Undergraduate Major'] || '').split(',').map(m => m.trim());
                    if (!majors.some(m => m === majorFilterValue)) {
                        return false;
                    }
                }

                // Industry filter
                if (industryFilterValue) {
                    const industry = categorizeIndustry(alumni['Current Company'], alumni['Role']);
                    if (industry !== industryFilterValue) {
                        return false;
                    }
                }

                // Coffee chat filter
                if (coffeeChatFilterValue) {
                    const coffeeChat = (alumni['Open to coffee chats?'] || '').toLowerCase();
                    if (coffeeChatFilterValue === 'yes' && coffeeChat !== 'yes' && coffeeChat !== 'online') {
                        return false;
                    }
                    if (coffeeChatFilterValue === 'no' && (coffeeChat === 'yes' || coffeeChat === 'online')) {
                        return false;
                    }
                }

                return true;
            });

            renderAlumni(filteredData);
        }

        // Add event listeners
        document.getElementById('majorFilter').addEventListener('change', applyFilters);
        document.getElementById('industryFilter').addEventListener('change', applyFilters);
        document.getElementById('coffeeChatFilter').addEventListener('change', applyFilters);

        // Initial render
        renderAlumni(filteredData);
    </script>
</body>
</html>
